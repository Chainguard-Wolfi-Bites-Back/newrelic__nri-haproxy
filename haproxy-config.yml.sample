integration_name: com.newrelic.haproxy

instances:
  - name: haproxy
    # Command can be all_data, metrics, or inventory
    command: all_data
    arguments:
      # HAProxy basic auth username. Only required if 
      # authentication is enabled.
      username: cacheuser
      # HAProxy basic auth password. Only required if 
      # authentication is enabled.
      password: password
